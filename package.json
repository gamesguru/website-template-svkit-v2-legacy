		"test": "playwright test"
	},
	"devDependencies": {
		"@playwright/test": "catalog:",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@tg-svelte/kit": "npm:@tg-svelte/kit@^2.50.1-tg.1",
		"@sveltejs/vite-plugin-svelte": "catalog:",
		"@vitejs/plugin-legacy": "catalog:",
		"prettier": "catalog:",
		"svelte": "catalog:",
		"svelte-check": "catalog:",
		"terser": "^5.0.0",
		"vite": "catalog:"
	},
	"overrides": {
		"@tg-svelte/kit": "file:../../packages/kit"
	},
	"pnpm": {}
}
